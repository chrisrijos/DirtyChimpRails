<div class="main-container">
    <div class="navbar">
        <p class="nav-li">
            <%= link_to 'Home', root_path %>
        </p>
         <p class="nav-li">
            <%= link_to 'Upload', video_new_path %>
        </p>
    </div>
        <div class="cards">
          <% @video.each do |v| %>
              <div class="card">
                <div class="card-image">
                  <img src="https://raw.githubusercontent.com/thoughtbot/refills/master/source/images/mountains.png" alt="">
                </div>
                <div class="card-header">
                  <%= v.title %>
                </div>
                <div class="card-copy">
                  <p><%= v.description %></p>
                   <p></p>
                </div>
              </div>
        <% end %>
    </div>
</div>
